{
  "version": 2,
  "env": {
    "DB_URI": "@DB_URI",
    "CORS_WHITELIST": "@CORS_WHITELIST"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
